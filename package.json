{
  "name": "@gashon/analytics",
  "version": "1.2.0",
  "description": "Analytics Component",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc --jsx react",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gashon/analytics-sdk.git"
  },
  "keywords": [
    "analytics",
    "react",
    "typescript"
  ],
  "author": "Gashon Hussein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gashon/analytics-sdk/issues"
  },
  "homepage": "https://github.com/gashon/analytics-sdk#readme",
  "devDependencies": {
    "@types/jest": "29.4.0",
    "@types/react": "^18.2.37",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.52.0",
    "eslint": "8.35.0",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.4.3",
    "prettier": "2.8.4",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^4.2.1",
    "omit-undefined": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1"
  }
}
